<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-GB">

    <CommandPrefix>Assistant</CommandPrefix>
    <Example>Assistant, show me all Announcements</Example>

    <Command Name="ShowMeAll">
      <Example>Show me all Announcements</Example>
      <ListenFor>Show [me] [all] {list}</ListenFor>
      <Feedback>Fetching your {list} list from SharePoint...</Feedback>
      <Navigate />
    </Command>

    <PhraseList Label="list">
      <Item>Announcements</Item>
    </PhraseList>

  </CommandSet>

</VoiceCommands>
